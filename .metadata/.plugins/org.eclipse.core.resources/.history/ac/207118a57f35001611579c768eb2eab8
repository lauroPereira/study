<?php
class TipoProduto{
	private $id;
	private $tipoProduto;
	
	public function setId($id){
		$this->id = $id;
	}
	public function getId(){
		return $this->id;
	}
	
	public function setTipoProduto($tipoProduto){
		$this->tipoProduto = $tipoProduto;
	}
	public function getTipoProduto(){
		return $this->tipoProduto;
	}
}